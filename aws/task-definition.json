{
  "family": "journeyguide-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::347277718006:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::347277718006:role/sourdough-task-role",
  "containerDefinitions": [
    {
      "name": "journeyguide",
      "image": "347277718006.dkr.ecr.us-west-2.amazonaws.com/journeyguide:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost/health || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/journeyguide",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": []
    }
  ]
}
